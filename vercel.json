{
    "build": {
        "src": "/api/app.py",
        "use": "@vercel/python"
    },
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/api/app.py"
        }
    ],
    "git": {
        "deploymentEnabled": {
            "main": true,
            "release": true
        }
    }
}